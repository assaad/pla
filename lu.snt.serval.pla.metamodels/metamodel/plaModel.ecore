<?xml version="1.0" encoding="UTF-8"?>
<ecore:EPackage xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore" name="pla"
    nsURI="http://plamodel/1.0" nsPrefix="pla">
  <eSubpackages name="framework" nsURI="" nsPrefix="framework">
    <eClassifiers xsi:type="ecore:EClass" name="Query">
      <eStructuralFeatures xsi:type="ecore:EReference" name="userProfile" eType="#//framework/UserProfile"
          containment="true"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="queryRequests" upperBound="-1"
          eType="#//framework/QueryRequest" containment="true"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="answers" upperBound="-1"
          eType="#//framework/Answer" containment="true"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="QueryRequest" abstract="true"/>
    <eClassifiers xsi:type="ecore:EClass" name="Answer" abstract="true">
      <eStructuralFeatures xsi:type="ecore:EReference" name="queryRequest" lowerBound="1"
          eType="#//framework/QueryRequest"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="UserProfile">
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="name" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="id" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"
          iD="true"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="QueryDataType" eSuperTypes="#//framework/QueryRequest #//framework/DataType">
      <eStructuralFeatures xsi:type="ecore:EReference" name="queriedDataType" lowerBound="1"
          eType="#//framework/DataType"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="requestedTimeBluring"
          eType="#//framework/TimeBluring"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="requestedValueBluring"
          eType="#//framework/ValueBluring"/>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="dateTime" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EDate"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="QueryPrivacyProfile" eSuperTypes="#//framework/QueryRequest">
      <eStructuralFeatures xsi:type="ecore:EReference" name="userProfile" eType="#//framework/UserProfile"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="queriedDataType" eType="#//framework/DataType"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="QueryCloaker" eSuperTypes="#//framework/QueryRequest">
      <eStructuralFeatures xsi:type="ecore:EReference" name="queriedDataType" lowerBound="1"
          eType="#//framework/QueryDataType"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="privacyPr" eType="#//framework/AnswerPrivacyProfile"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="privacyRule" eType="#//framework/PrivacyACRule"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="PrivacyProfile">
      <eStructuralFeatures xsi:type="ecore:EReference" name="privacyRules" upperBound="-1"
          eType="#//framework/PrivacyACRule" containment="true"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="userProfile" lowerBound="1"
          eType="#//framework/UserProfile"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="DataType">
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="location" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="name" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="PrivacyACRule">
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="cloakingAlgorithm" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="dataType" lowerBound="1"
          eType="#//framework/DataType"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="timeBluring" eType="#//framework/TimeBluring"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="valueBluring" eType="#//framework/ValueBluring"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="Bluring" abstract="true"/>
    <eClassifiers xsi:type="ecore:EClass" name="TimeBluring" abstract="true" eSuperTypes="#//framework/Bluring">
      <eStructuralFeatures xsi:type="ecore:EReference" name="EReference0" eType="#//framework/Bluring"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="ValueBluring" abstract="true" eSuperTypes="#//framework/Bluring"/>
    <eClassifiers xsi:type="ecore:EClass" name="AnswerDataType" abstract="true" eSuperTypes="#//framework/Answer">
      <eStructuralFeatures xsi:type="ecore:EReference" name="resultedTimeBluring"
          eType="#//framework/TimeBluring"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="ResultedValueBluring"
          eType="#//framework/ValueBluring"/>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="dateTime" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EDate"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="AnswerPrivacyProfile" eSuperTypes="#//framework/Answer #//framework/PrivacyACRule">
      <eStructuralFeatures xsi:type="ecore:EReference" name="relatedPrivacyProfile"
          eType="#//framework/PrivacyProfile"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="relatedPrivacyRule" lowerBound="1"
          eType="#//framework/PrivacyACRule"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="IntervalAnswer" eSuperTypes="#//framework/AnswerDataType">
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="min" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EDouble"/>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="max" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EDouble"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="ValueAnswer" eSuperTypes="#//framework/AnswerDataType">
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="value" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EDouble"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="IntervalBluring" eSuperTypes="#//framework/ValueBluring">
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="IntervalLength" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EDouble"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="NoiseBluring" eSuperTypes="#//framework/ValueBluring">
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="variance" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EDouble"/>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="TypeOfNoise" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="TimeAveraging" eSuperTypes="#//framework/TimeBluring">
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="duration" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EDouble"/>
    </eClassifiers>
  </eSubpackages>
</ecore:EPackage>
